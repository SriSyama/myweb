pipeline{
    agent any
     tools {
        maven 'Maven-3.6.3'
    }
    
    stages{
        stage("Git Checkout"){
            steps{
                git url: 'https://github.com/srinivas1987devops/myweb.git'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean sonar:sonar package"
                
            }
        }
        
    }
}
